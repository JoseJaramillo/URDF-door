<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="door">

 <link name="world"/>
  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="baseLink"/>
    <origin rpy= "0 0 1.571" xyz="0 0 0"/>
  </joint>

<link name="baseLink">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.048966669" ixy="0" ixz="0" iyy="0.046466667" iyz="0" izz="0.0041666669"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <mesh filename="package://reemc_door/models/meshes/frame2.stl"/>
       </geometry>
	   <material name="doormat0">
      <color rgba="0.8 0.8 0.3 1" />
      </material>

	  </visual>
     <collision>
       <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <mesh filename="package://reemc_door/models/meshes/frame2.stl"/>
       </geometry>
     </collision>

   </link>
   <link name="door">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.048966669" ixy="0" ixz="0" iyy="0.046466667" iyz="0" izz="0.0041666669"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <mesh filename="package://reemc_door/models/meshes/door2.stl"/>
       </geometry>
	   <material name="doormat0">
      <color rgba="0.8 0.8 0.3 1" />
      </material>

	  </visual>
     <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <mesh filename="package://reemc_door/models/meshes/door2.stl"/>
       </geometry>
     </collision>

   </link>

   
   <joint name="joint_baseLink_door" type="continuous">
    <parent link="baseLink"/>
    <child link="door"/>
	<dynamics damping="1.0" friction="0.0001"/>
    <origin xyz="0.4155 0.0597 0"/>
		<axis xyz="0 0 1"/>

  </joint>

  <link name="lever">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.048966669" ixy="0" ixz="0" iyy="0.046466667" iyz="0" izz="0.0041666669"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <mesh filename="package://reemc_door/models/meshes/lever2.stl"/>
       </geometry>
	   <material name="doormat0">
      <color rgba="0.8 0.8 0.3 1" />
      </material>

	  </visual>
     <collision>
       <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <mesh filename="package://reemc_door/models/meshes/lever2.stl"/>
       </geometry>
     </collision>

   </link>

   
   <joint name="joint_door_lever" type="continuous">
    <parent link="door"/>
    <child link="lever"/>
	<dynamics damping="1.0" friction="0.0001"/>
    <origin rpy="1.5708 0 0" xyz="-0.7455 -0.0705 1"/>
		<axis xyz="0 0 1"/>

  </joint>
</robot>
